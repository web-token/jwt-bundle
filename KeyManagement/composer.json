{
    "name": "web-token/jwt-key-mgmt-bundle",
    "description": "JWT Key Management Bundle of the JWT Framework.",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["JWS", "JWT", "JWE", "JWA", "JWK", "JWKSet", "Jot", "Jose", "RFC7515", "RFC7516", "RFC7517", "RFC7518", "RFC7519", "RFC7520", "Bundle", "Symfony"],
    "homepage": "https://github.com/web-token/jwt-key-mgmt-bundle",
    "authors": [
        {
            "name": "Florent Morselli",
            "homepage": "https://github.com/Spomky"
        },{
            "name": "All contributors",
            "homepage": "https://github.com/web-token/jwt-key-mgmt-bundle/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jose\\Bundle\\KeyManagement\\":  ""
        }
    },
    "require": {
        "php": "^7.1",
        "symfony/routing": "^3.3|^4.0",
        "web-token/jwt-key-mgmt": "^0.3",
        "web-token/jwt-bundle": "^0.3"
    },
    "require-dev": {
        "symfony/framework-bundle": "^3.3|^4.0",
        "symfony/yaml": "^3.3|^4.0",
        "symfony/browser-kit": "^3.3|^4.0",
        "phpunit/phpunit": "^6.0",
        "php-http/httplug-bundle": "^1.7",
        "php-http/mock-client": "^1.0"
    },
    "suggest": {
        "php-http/httplug-bundle": "To enable JKU/X5U support."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
